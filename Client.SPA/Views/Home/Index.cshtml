@model Client.SPA.DTO.UserDto;

<app-root params="history: history, basename: basename"></app-root>

@section scripts {
    <script>
        window.User = {
            Token: '@Model.Token',
            TokenExpirationTime: '@Model.TokenExpirationTime'
        }
    </script>

    <script>window.apiAccessToken = '@Model.Token'</script>
    <script src="~/dist/main.js" asp-append-version="true"></script>
}
